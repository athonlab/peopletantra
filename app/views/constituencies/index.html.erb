<h1>Listing constituencies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Member Name</th>
    <th>Code</th>
    <th>State</th>
  </tr>

<% for constituency in @constituencies %>
  <tr>
    <td><%=h constituency.name %></td>
    <td><%=h constituency.member %></td>
    <td><%=h constituency.code %></td>
    <td><%=h constituency.state %></td>
    <td><%= link_to 'Show', constituency %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New constituency', new_constituency_path %>
